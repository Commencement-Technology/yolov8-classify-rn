{
  "expo": {
    "plugins": [
      [
        "../app.plugin.js",
        {
          "cameraUsageDescription": "Camera used for capturing classification input"
        }
      ]
    ],
    "name": "yolov8-classify-example",
    "slug": "yolov8-classify-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#f0f0f0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "expo.modules.yolov8classify.example"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "expo.modules.yolov8classify.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
